<!DOCTYPE html>
<html>
<head>
    <title>Users</title>
    <style>
        body { font-family: Arial, sans-serif; }
        .user { border: 1px solid #ccc; padding: 10px; margin: 10px; }
    </style>
</head>
<body>
    <h1>Users</h1>
    <% users.forEach(user => { %>
        <div class="user">
            <h2><%= user.name %></h2>
            <p>Email: <%= user.email %></p>
            <p>Age: <%= user.age %></p>
        </div>
    <% }) %>
</body>
</html>
