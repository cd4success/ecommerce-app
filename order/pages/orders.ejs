<!DOCTYPE html>
<html>
<head>
    <title>Orders</title>
    <style>
        body { font-family: Arial, sans-serif; }
        .order { border: 1px solid #ccc; padding: 10px; margin: 10px; }
    </style>
</head>
<body>
    <h1>Orders</h1>
    <% orders.forEach(order => { %>
        <div class="order">
            <h2>Order ID: <%= order.id %></h2>
            <p>Product ID: <%= order.productId %></p>
            <p>Quantity: <%= order.quantity %></p>
        </div>
    <% }) %>
</body>
</html>
